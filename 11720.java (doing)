import java.util.Scanner;

class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int cnt = 0;
        int forCalc = 0;
        String forCalcLengthString = "";
        int forSum = 0;
        int finalSum = 0;
        cnt = sc.nextInt();

        if (cnt >= 1 && cnt <= 100) {
            forCalc = sc.nextInt();
            forCalcLengthString = String.valueOf(forCalc);
            String[] forCalcArr = new String[forCalcLengthString.length()];

            for (int i = 0; i < forCalcArr.length; i++) {
                forSum = Integer.parseInt(forCalcArr[i]);
                // finalSum += forSum;
                System.out.println(forSum);
            }
        }
    }
}
